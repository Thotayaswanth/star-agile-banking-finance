pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                // Get some code from a GitHub repository
                git 'https://github.com/Thotayaswanth/star-agile-banking-finance.git'

                // Run Maven on a Unix agent.
                sh "mvn -Dmaven.test.failure.ignore=true clean package"

            }        
        }
         stage('Create Docker Image') {
           steps {
               sh 'docker build -t thotayaswanth/financeme:1.0.'
                    }
   }             }
    }
